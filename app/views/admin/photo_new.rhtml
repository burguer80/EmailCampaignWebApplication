<%= error_messages_for :photo %>

<% form_for(:photo, :url => {:action => "photo_create",:ideflyer =>  @eflyer, :type => 1 }, 
                      :html => { :multipart => true })   do |f|-%>
  <p>
    <label for="photo">IMAGEN: </label>
        <%= f.file_field :uploaded_data %><br> 603px x 528px

  </p>
  <p>
    <%= submit_tag 'Subir' %>
  </p>
<% end -%>
<br>
  
